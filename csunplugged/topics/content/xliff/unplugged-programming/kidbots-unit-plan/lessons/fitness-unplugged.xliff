      <trans-unit id="13144" identifier="5695ec7ebb7dde9dbb96e1b572062e50">
        <source>Who designs programming languages for computers?</source>
        <target>crwdns13144:0crwdne13144:0</target>
        <note>Context: Paragraph text&#13;
XPath: /p[2]</note>
      </trans-unit>
      <trans-unit id="13145" identifier="4460fd52c3ef567c91cd109657e20025">
        <source>Lesson starter</source>
        <target>crwdns13145:0crwdne13145:0</target>
        <note>Context: Headline 2&#13;
XPath: /h2[2]</note>
      </trans-unit>
      <trans-unit id="13146" identifier="d9014380796394fce3e46229b3ead590">
        <source>{panel type=&quot;general&quot; title=&quot;Notes on resources&quot;}</source>
        <target>crwdns13146:0crwdne13146:0</target>
        <note>Context: Paragraph text&#13;
XPath: /p[3]</note>
      </trans-unit>
      <trans-unit id="13147" identifier="c29bd694b856a5f79b5a834613067693">
        <source>The resources we listed are for groups of 3 students.</source>
        <target>crwdns13147:0crwdne13147:0</target>
        <note>Context: Paragraph text&#13;
XPath: /p[4]</note>
      </trans-unit>
      <trans-unit id="13148" identifier="0195c2d106e4d52b36bca78ff4c3e7a4">
        <source>{panel end}</source>
