      <trans-unit id="15153" identifier="2e3d98f5b49eb56022fa298c48210674">
        <source>Press &quot;home&quot; and then &quot;clear&quot; to try again.] for (2) secs
        stop [all v]
      else
        move (-50) steps
        wait (0.5) secs
      end
    else
      if &amp;lt;(statement) = [turn left]&amp;gt; then
        turn ccw (90) degrees
        wait (0.5) secs
      else
        if &amp;lt;(statement) = [turn right]&amp;gt; then
          turn cw (90) degrees
          wait (0.5) secs
        else
          if &amp;lt;(statement) = [spin]&amp;gt; then
            repeat (20)
              turn cw (90) degrees
            end
          end
        end
      end
    end
  end
  change [source line v] by (1)
end
if &amp;lt;touching [Mouse v] ?&amp;gt; then
  play sound [meow v]
  say [Gotcha!] for (2) secs
else
  say [Oh no!</source>
